    ApmathAzureCloudLarisa.Controllers.HomeController obj = new ApmathAzureCloudLarisa.Controllers.HomeController();
    //double[] d = obj.Haha();
    string res = "";
    double[,] x = new double[3, 3];


    if (IsPost)
    {
        for (int i = 0; i < 3; i++)
        {
            for (int j = 0; j < 3; j++)
            {
                x[i, j] = Convert.ToDouble(Request.Form[i.ToString() +"+"+ j.ToString()]);
            }
        }
    }
    double[] d = obj.Haha(x);
    for (int i = 0; i < d.Length; i++)
    {
        res += d[i].ToString() + " ";
    }
}

